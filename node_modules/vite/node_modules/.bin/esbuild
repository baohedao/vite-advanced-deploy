#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/ABB/Myvite/vite-advanced-deploy/node_modules/.pnpm/esbuild@0.14.38/node_modules/esbuild/bin/node_modules:/mnt/d/ABB/Myvite/vite-advanced-deploy/node_modules/.pnpm/esbuild@0.14.38/node_modules/esbuild/node_modules:/mnt/d/ABB/Myvite/vite-advanced-deploy/node_modules/.pnpm/esbuild@0.14.38/node_modules:/mnt/d/ABB/Myvite/vite-advanced-deploy/node_modules/.pnpm/node_modules:/mnt/d/ABB/Myvite/vite-advanced-deploy/node_modules:/mnt/d/ABB/Myvite/node_modules:/mnt/d/ABB/node_modules:/mnt/d/node_modules:/mnt/d/ABB/Myvite/vite-advanced-deploy/node_modules/.pnpm/vite@2.9.6/node_modules/vite/node_modules:/mnt/d/ABB/Myvite/vite-advanced-deploy/node_modules/.pnpm/vite@2.9.6/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/ABB/Myvite/vite-advanced-deploy/node_modules/.pnpm/esbuild@0.14.38/node_modules/esbuild/bin/node_modules:/mnt/d/ABB/Myvite/vite-advanced-deploy/node_modules/.pnpm/esbuild@0.14.38/node_modules/esbuild/node_modules:/mnt/d/ABB/Myvite/vite-advanced-deploy/node_modules/.pnpm/esbuild@0.14.38/node_modules:/mnt/d/ABB/Myvite/vite-advanced-deploy/node_modules/.pnpm/node_modules:/mnt/d/ABB/Myvite/vite-advanced-deploy/node_modules:/mnt/d/ABB/Myvite/node_modules:/mnt/d/ABB/node_modules:/mnt/d/node_modules:/mnt/d/ABB/Myvite/vite-advanced-deploy/node_modules/.pnpm/vite@2.9.6/node_modules/vite/node_modules:/mnt/d/ABB/Myvite/vite-advanced-deploy/node_modules/.pnpm/vite@2.9.6/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../esbuild@0.14.38/node_modules/esbuild/bin/esbuild" "$@"
else
  exec node  "$basedir/../../../../../esbuild@0.14.38/node_modules/esbuild/bin/esbuild" "$@"
fi
